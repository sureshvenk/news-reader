# Server Configuration
NODE_ENV=development
PORT=5000

# News API Configuration (The News API)
NEWS_API_KEY=your_news_api_key_here
NEWS_API_BASE_URL=https://api.thenewsapi.com/v1

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
CACHE_TTL=600

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=http://localhost:5173

# Logging
LOG_LEVEL=info
